{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "UpdateContributionRequest.json",
  "type": "object",
  "title": "Update Contribution Request",
  "description": "Data contract for updating contribution records via the Court Data API",
  "properties": {
    "id": {
      "type": "integer",
      "description": "The ID of the contribution record"
    },
    "userModified": {
      "type": "string",
      "description": "The user modifying the record"
    }
  },
  "extends": {
    "$ref": "ContributionRequest.json"
  },
  "additionalProperties": false,
  "required": ["id", "userModified"]
}
